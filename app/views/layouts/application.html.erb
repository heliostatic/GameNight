<!DOCTYPE html>
<html>
<head>
  <title>Tempe12</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
	<div id="container">
		<%= render 'layouts/header' %>
		<section>
		<% flash.each do |name, msg| %>
			<%= content_tag :div, msg, :id => "flash_#{name}" %>
		<% end %>
		
		<%= yield %>
		</section>
	</div>

</body>
</html>
